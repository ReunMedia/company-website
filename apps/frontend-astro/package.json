{
  "name": "frontend-astro",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm codegen && pnpm build:scripts && astro build",
    "build:scripts": "esbuild src/scripts/setInitialTheme.ts --minify --outfile=public/static/dist/setInitialTheme.js",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "dev": "concurrently -p command \"pnpm build:scripts\" \"pnpm codegen:watch\" \"astro dev\"",
    "docker:test": "docker compose -f docker-compose.yaml -f docker-compose.test.yaml --env-file .env up -d --build",
    "lint": "concurrently -p command \"npmPkgJsonLint .\" \"prettier . --check\" \"eslint\" \"astro check\"",
    "preview": "astro preview",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/vue": "^5.0.13",
    "@braintree/sanitize-url": "^7.1.1",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource/inter": "^5.2.5",
    "@fontsource/poppins": "^5.2.6",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "^4.8.1",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/simple-icons": "^1.2.34",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@reunmedia/astro-og-images": "^0.2.0",
    "@shikijs/rehype": "^3.4.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.6",
    "@urql/core": "^5.1.1",
    "@urql/vue": "^1.4.3",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.1.0",
    "astro": "^5.7.12",
    "astro-icon": "^1.1.5",
    "astro-seo": "^0.8.4",
    "deepmerge-ts": "^7.1.5",
    "dotenv": "^16.5.0",
    "es-toolkit": "^1.37.2",
    "esbuild": "^0.25.4",
    "graphql": "^16.11.0",
    "pupa": "^3.1.0",
    "rehype-headings-normalize": "^0.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-smartypants": "^3.0.2",
    "sanitize-html": "^2.16.0",
    "sharp": "^0.34.1",
    "shiki": "^3.4.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.6",
    "ts-extras": "^0.14.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@parcel/watcher": "^2.5.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/parser": "^8.32.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "htm": "^3.1.1",
    "npm-package-json-lint": "^8.0.0",
    "npm-package-json-lint-config-default": "^7.0.1",
    "preact": "^10.26.6",
    "preact-render-to-string": "^6.5.13",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-packagejson": "^2.5.12",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.5.2+sha512.da9dc28cd3ff40d0592188235ab25d3202add8a207afbedc682220e4a0029ffbff4562102b9e6e46b4e3f9e8bd53e6d05de48544b0c57d4b0179e22c76d1199b",
  "engines": {
    "node": "^22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp"
    ]
  }
