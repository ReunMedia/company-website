{
  "name": "frontend-astro",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm codegen && pnpm build:scripts && astro build",
    "build:scripts": "esbuild src/scripts/setInitialTheme.ts --minify --outfile=public/static/dist/setInitialTheme.js",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "dev": "concurrently -p command \"pnpm build:scripts\" \"pnpm codegen:watch\" \"astro dev\"",
    "docker:test": "docker compose -f docker-compose.yaml -f docker-compose.test.yaml --env-file .env up -d --build",
    "lint": "concurrently -p command \"npmPkgJsonLint .\" \"prettier . --check\" \"eslint\" \"astro check\"",
    "preview": "astro preview",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/vue": "^5.1.3",
    "@braintree/sanitize-url": "^7.1.1",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/poppins": "^5.2.7",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/simple-icons": "^1.2.63",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@reunmedia/astro-normalize-trailing-slash": "^0.1.1",
    "@reunmedia/astro-og-images": "^0.2.0",
    "@shikijs/rehype": "^3.20.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@urql/core": "^6.0.1",
    "@urql/vue": "^2.0.0",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.1.0",
    "astro": "^5.16.6",
    "astro-icon": "^1.1.5",
    "astro-seo": "^0.8.4",
    "deepmerge-ts": "^7.1.5",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.43.0",
    "esbuild": "^0.27.2",
    "graphql": "^16.12.0",
    "pupa": "^3.3.0",
    "rehype-headings-normalize": "^0.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-smartypants": "^3.0.2",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.34.5",
    "shiki": "^3.20.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "ts-extras": "^0.16.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vee-validate": "^4.15.1",
    "vite": "^6.4.1",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@parcel/watcher": "^2.5.1",
    "@reunmedia/eslint-config": "^1.8.1",
    "@types/eslint-config-prettier": "^6.11.3",
